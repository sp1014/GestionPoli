) return null;\n      return this.chartsLib.charts.Bar.convertOptions({\n        chart: {\n          title: 'Company Performance',\n          subtitle: 'Sales, Expenses, and Profit: 2014-2017'\n        },\n        bars: 'vertical',\n        vAxis: {\n          format: 'decimal'\n        },\n        height: 400,\n        width: '100%',\n        colors: [primary, secondary, '#51bb25']\n      });\n    },\n    horizontalChartOptions: function horizontalChartOptions() {\n      if (!this.chartsLib) return null;\n      return this.chartsLib.charts.Bar.convertOptions({\n        chart: {\n          title: 'Company Performance',\n          subtitle: 'Sales, Expenses, and Profit: 2014-2017'\n        },\n        bars: 'horizontal',\n        vAxis: {\n          format: 'decimal'\n        },\n        height: 400,\n        width: '100%',\n        colors: [primary, secondary, '#51bb25']\n      });\n    }\n  },\n  methods: {\n    onChartReady: function onChartReady(chart, google) {\n      this.chartsLib = google;\n    }\n  }\n};","map":{"version":3,"mappings":"AAkNA;AACA;AAEA;AAEA;EACAA;IAAAC;EAAA,CADA;EAEAC,IAFA,kBAEA;IACA;MACAC,eADA;MAEAC;QACAC,cACA,yBADA,EAEA,WAFA,EAGA,WAHA,EAIA,eAJA,EAKA,gBALA,EAMA,aANA,CADA;QASAC,cACA,yBADA,EAEA,WAFA,EAGA,UAHA,EAIA,eAJA,EAKA,eALA,EAMA,YANA,CATA;QAiBAC,cACA,sCADA,EAEA,gBAFA,EAGA,eAHA,EAIA,aAJA,EAKA,cALA,EAMA,gBANA,EAOA,cAPA,EAQA,eARA,EASA,iBATA,EAUA,cAVA,EAWA,gBAXA,EAYA,iBAZA,EAaA,iBAbA,EAcA,eAdA,EAeA,eAfA,EAgBA,aAhBA,EAiBA,eAjBA,EAkBA,kBAlBA,EAmBA,iBAnBA,EAoBA,eApBA,EAqBA,aArBA,EAsBA,cAtBA,EAuBA,YAvBA,CAjBA;QA0CAC;UACAC,4BADA;UAEAC,aAFA;UAGAC,WAHA;UAIAC;QAJA,CA1CA;QAgDAC;UACAJ,4BADA;UAEAK,UAFA;UAGAJ,aAHA;UAIAC,WAJA;UAKAC;QALA,CAhDA;QAuDAG;UACAN,4BADA;UAEAO,cAFA;UAGAN,aAHA;UAIAC,WAJA;UAKAM,qBALA;UAMAC;YAAA;cAAAC;YAAA;YACA;cAAAA;YAAA,CADA;YAEA;cAAAA;YAAA,CAFA;YAGA;cAAAA;YAAA;UAHA,CANA;UAWA;UACA;UACAP;QAbA;MAvDA,CAFA;MAyEAQ;QACAf,cACA,6BADA,EAEA,mBAFA,EAGA,mBAHA,EAIA,mBAJA,EAKA,mBALA,CADA;QAQAC,cACA,+CADA,EAEA,6BAFA,EAGA,4BAHA,EAIA,8BAJA,EAKA,6BAL