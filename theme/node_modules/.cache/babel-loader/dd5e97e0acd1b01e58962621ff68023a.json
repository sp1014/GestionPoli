{"ast":null,"code":"import \"core-js/modules/es.json.stringify.js\";\nimport VueBootstrapTypeahead from 'vue-bootstrap-typeahead';\n\nvar axios = require('axios');\n\nexport default {\n  components: {\n    VueBootstrapTypeahead: VueBootstrapTypeahead\n  },\n  filters: {\n    stringify: function stringify(value) {\n      return JSON.stringify(value, null, 2);\n    }\n  },\n  data: function data() {\n    return {\n      query: '',\n      selectedUser: null,\n      searchUser: null,\n      users: [],\n      displayUser: false,\n      countries: ['alabama', 'alaska', 'arizona', 'arkansas', 'california', 'colorado', 'connecticut', 'delaware', 'florida', 'georgia', 'hawaii', 'idaho', 'illinois', 'indiana', 'iowa', 'kansas', 'kentucky', 'louisiana', 'maine', 'maryland', 'massachusetts', 'michigan', 'minnesota', 'mississippi', 'missouri', 'montana', 'nebraska', 'nevada', 'new hampshire', 'new jersey', 'new mexico', 'new york', 'north carolina', 'north dakota', 'ohio', 'oklahoma', 'oregon', 'pennsylvania', 'rhode island', 'south carolina', 'south dakota', 'tennessee', 'texas', 'utah', 'vermont', 'virginia', 'washington', 'west virginia', 'wisconsin', 'wyoming']\n    };\n  },\n  watch: {\n    query: function query(newQuery) {\n      var _this = this;\n\n      axios.get(\"https://api.github.com/search/users?q=\".concat(newQuery)).then(function (res) {\n        _this.users = res.data.items;\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";AAuHA;;AAEA;;AACA;EACAA;IACAC;EADA,CADA;EAIAC;IACAC,SADA,qBACAC,KADA,EACA;MACA;IACA;EAHA,CAJA;EASAC,IATA,kBASA;IACA;MACAC,SADA;MAEAC,kBAFA;MAGAC,gBAHA;MAIAC,SAJA;MAKAC,kBALA;MAMAC,YACA,SADA,EAEA,QAFA,EAGA,SAHA,EAIA,UAJA,EAKA,YALA,EAMA,UANA,EAOA,aAPA,EAQA,UARA,EASA,SATA,EAUA,SAVA,EAWA,QAXA,EAYA,OAZA,EAaA,UAbA,EAcA,SAdA,EAeA,MAfA,EAgBA,QAhBA,EAiBA,UAjBA,EAkBA,WAlBA,EAmBA,OAnBA,EAoBA,UApBA,EAqBA,eArBA,EAsBA,UAtBA,EAuBA,WAvBA,EAwBA,aAxBA,EAyBA,UAzBA,EA0BA,SA1BA,EA2BA,UA3BA,EA4BA,QA5BA,EA6BA,eA7BA,EA8BA,YA9BA,EA+BA,YA/BA,EAgCA,UAhCA,EAiCA,gBAjCA,EAkCA,cAlCA,EAmCA,MAnCA,EAoCA,UApCA,EAqCA,QArCA,EAsCA,cAtCA,EAuCA,cAvCA,EAwCA,gBAxCA,EAyCA,cAzCA,EA0CA,WA1CA,EA2CA,OA3CA,EA4CA,MA5CA,EA6CA,SA7CA,EA8CA,UA9CA,EA+CA,YA/CA,EAgDA,eAhDA,EAiDA,WAjDA,EAkDA,SAlDA;IANA;EA2DA,CArEA;EAsEAC;IACAN,KADA,iBACAO,QADA,EACA;MAAA;;MACAC,MACAC,GADA,iDACAF,QADA,GAEAG,IAFA,CAEA;QACA;MACA,CAJA;IAKA;EAPA;AAtEA","names":["components","VueBootstrapTypeahead","filters","stringify","value","data","query","selectedUser","searchUser","users","displayUser","countries","watch","newQuery","axios","get","then"],"sourceRoot":"src/pages/form_widgets","sources":["typeahead.vue"],"sourcesContent":["<template>\n  <div>\n    <Breadcrumbs main=\"Form Widgets\" title=\"Typeahead\" />\n    <!-- Container-fluid starts-->\n    <div class=\"container-fluid\">\n      <div class=\"typeahead\">\n        <div class=\"row\">\n          <div class=\"col-md-12 col-xl-6\">\n            <div class=\"card\">\n              <div class=\"card-header\">\n                <h5>Basic Demo</h5>\n                <span>This is the simple demo for Typeahead.</span>\n              </div>\n              <div class=\"card-body\">\n                <div id=\"the-basics\">\n                  <form class=\"theme-form\">\n                    <div class=\"form-group\">\n                      <vue-bootstrap-typeahead\n                        v-model=\"query\"\n                        inputClass=\"typeahead\"\n                        :data=\"countries\"\n                        placeholder=\"State of USA\"\n                      />\n                    </div>\n                  </form>\n                </div>\n              </div>\n            </div>\n          </div>\n\n          <div class=\"col-sm-12 col-xl-6\">\n            <div class=\"card\">\n              <div class=\"card-header\">\n                <h5>RTL Support</h5>\n                <span>Dataset shows in Right Side.</span>\n              </div>\n              <div class=\"card-body\">\n                <div id=\"rtl-support\">\n                  <form class=\"theme-form\">\n                    <div class=\"form-group\">\n                      <vue-bootstrap-typeahead\n                        v-model=\"query\"\n                        inputClass=\"typeahead\"\n                        :data=\"countries\"\n                        dir=\"rtl\"\n                        placeholder=\"Countries\"\n                      />\n                    </div>\n                  </form>\n                </div>\n              </div>\n            </div>\n          </div>\n\n          <div class=\"col-sm-12 col-xl-6\">\n            <div class=\"card\">\n              <div class=\"card-header\">\n                <h5>API</h5>\n                <span>Using API fetch Typeahead Data</span>\n              </div>\n              <div class=\"card-body\">\n                <div id=\"prefetch\">\n                  <!-- <form class=\"theme-form\"> -->\n                  <div class=\"form-group mb-0\">\n                    <vue-bootstrap-typeahead\n                      class=\"mb-4\"\n                      v-model=\"query\"\n                      :data=\"users\"\n                      :serializer=\"item => item.login\"\n                      @hit=\"selectedUser = $event\"\n                      placeholder=\"Search GitHub Users\"\n                    />\n                    <h6>Selected User JSON</h6>\n                    <pre v-if=\"selectedUser\" class=\"mb-0\">{{ selectedUser | stringify }}</pre>\n                  </div>\n                  <!-- </form> -->\n                </div>\n              </div>\n            </div>\n          </div>\n\n          <div class=\"col-sm-12 col-xl-6\">\n            <div class=\"card\">\n              <div class=\"card-header\">\n                <h5>Prepending & Appending</h5>\n                <span>Add Prepending & Appending lable or button in typerhead as well as result data on button click</span>\n              </div>\n              <div class=\"card-body\">\n                <div id=\"bloodhound\">\n                  <div class=\"form-group mb-0\">\n                    <vue-bootstrap-typeahead\n                      class=\"mb-4\"\n                      v-model=\"query\"\n                      :data=\"users\"\n                      :serializer=\"item => item.login\"\n                      @hit=\"searchUser = $event\"\n                      placeholder=\"Search GitHub Users\"\n                      prepend=\"name:\"\n                    >\n                      <template slot=\"append\">\n                        <button @click=\"displayUser=true\" class=\"btn btn-primary\">Search</button>\n                      </template>\n                    </vue-bootstrap-typeahead>\n\n                    <h6>Search User JSON</h6>\n                    <pre v-if=\"displayUser\" class=\"mb-0\">{{ searchUser | stringify }}</pre>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- Container-fluid Ends-->\n  </div>\n</template>\n\n<script>\n  import VueBootstrapTypeahead from 'vue-bootstrap-typeahead';\n  \n  const axios = require('axios');\n  export default {\n    components: {\n      VueBootstrapTypeahead\n    },\n    filters: {\n      stringify(value) {\n        return JSON.stringify(value, null, 2);\n      }\n    },\n    data() {\n      return {\n        query: '',\n        selectedUser: null,\n        searchUser:null,\n        users: [],\n        displayUser: false,\n        countries: [\n          'alabama',\n          'alaska',\n          'arizona',\n          'arkansas',\n          'california',\n          'colorado',\n          'connecticut',\n          'delaware',\n          'florida',\n          'georgia',\n          'hawaii',\n          'idaho',\n          'illinois',\n          'indiana',\n          'iowa',\n          'kansas',\n          'kentucky',\n          'louisiana',\n          'maine',\n          'maryland',\n          'massachusetts',\n          'michigan',\n          'minnesota',\n          'mississippi',\n          'missouri',\n          'montana',\n          'nebraska',\n          'nevada',\n          'new hampshire',\n          'new jersey',\n          'new mexico',\n          'new york',\n          'north carolina',\n          'north dakota',\n          'ohio',\n          'oklahoma',\n          'oregon',\n          'pennsylvania',\n          'rhode island',\n          'south carolina',\n          'south dakota',\n          'tennessee',\n          'texas',\n          'utah',\n          'vermont',\n          'virginia',\n          'washington',\n          'west virginia',\n          'wisconsin',\n          'wyoming'\n        ]\n      };\n    },\n    watch: {\n      query(newQuery) {\n        axios\n          .get(`https://api.github.com/search/users?q=${newQuery}`)\n          .then(res => {\n            this.users = res.data.items;\n          });\n      }\n    }\n  };\n</script>"]},"metadata":{},"sourceType":"module"}