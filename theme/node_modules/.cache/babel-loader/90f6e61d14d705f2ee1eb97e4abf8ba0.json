ent() {\n      if (this.counter < this.quickViewProduct.stock) this.counter++;\n    },\n    decrement: function decrement() {\n      if (this.counter > 1) this.counter--;\n    },\n    //Grid changes\n    grid2: function grid2() {\n      this.col2 = true;\n      this.col3 = false;\n      this.col4 = false;\n      this.col6 = false;\n      this.listViewEnable = false;\n    },\n    grid3: function grid3() {\n      this.col2 = false;\n      this.col3 = true;\n      this.col4 = false;\n      this.col6 = false;\n      this.listViewEnable = false;\n    },\n    grid4: function grid4() {\n      this.col2 = false;\n      this.col3 = false;\n      this.col4 = true;\n      this.col6 = false;\n      this.listViewEnable = false;\n    },\n    grid6: function grid6() {\n      this.col2 = false;\n      this.col3 = false;\n      this.col4 = false;\n      this.col6 = true;\n      this.listViewEnable = false;\n    },\n    listView: function listView() {\n      this.listViewEnable = true;\n      this.list = true;\n      this.col2 = false;\n      this.col3 = false;\n      this.col4 = false;\n      this.col6 = false;\n    },\n    gridView: function gridView() {\n      this.listViewEnable = false;\n      this.col4 = true;\n    }\n  }\n};","map":{"version":3,"mappings":";AAuPA;AACA;AAEA;EACAA,eADA;EAEAC;IACAC;EADA,CAFA;EAKAC,IALA,kBAKA;IACA;MACAC,gBADA;MAEAC,oBAFA;MAGAC,UAHA;MAIAC,cAJA;MAKAC,cALA;MAMAC,SANA;MAOAC,eAPA;MAQAC,WARA;MASAC,WATA;MAUAC,UAVA;MAWAC,WAXA;MAYAC,qBAZA;MAaAC;IAbA;EAeA,CArBA;EAsBAC,4BACAC;IACAC,wCADA;IAEAC;EAFA,EADA,CAtBA;EA4BAC;IAEA;IACAC,SAHA,qBAGAC,IAHA,EAGA;MACA;IACA,CALA;IAOA;IACAC,YARA,wBAQAC,KARA,EAQA;MACA;IACA,CAVA;IAYA;IACAC,SAbA,qBAaAC,WAbA,EAaA;MACA;MACA;IACA,CAhBA;IAkBAC,cAlBA,4BAkBA;MACA;IACA,CApBA;IAsBA;IACAC,gBAvBA,4BAuBAC,IAvBA,EAuBA;MACA;IACA,CAzBA;IA2BA;IACAC;MACAC;MACA;IACA,CA/BA;IAiCA;IACAC;MACA;MACA;IACA,CArCA;IAsCAC;MACA;IACA,CAxCA;IA0CA;IACAC,SA3CA,uBA2CA;MACA;IACA,CA7CA;IA+CAC,SA/CA,uBA+CA;MACA;IACA,CAjDA;IAmDA;IACAC,KApDA,mBAoDA;MACA;MACA;MACA;MA